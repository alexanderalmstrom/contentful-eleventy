{
  "name": "eleventy",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Panagora",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "eleventy": "npx eleventy",
    "eleventy:dev": "npx eleventy --watch",
    "styles:dev": "node-sass src/styles/ --output dist/styles --source-map true --source-map-contents true",
    "styles:prod": "node-sass src/styles/ --output dist/styles",
    "scripts:dev": "webpack --mode=development",
    "scripts:prod": "webpack --mode=production",
    "server": "light-server -c .lightserverrc",
    "start": "npm-run-all clean styles:dev scripts:dev eleventy --parallel eleventy:dev server --print-label",
    "build": "run-s styles:prod scripts:prod eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "contentful": "^7.13.1",
    "dotenv": "^8.2.0",
    "light-server": "^2.6.4",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
